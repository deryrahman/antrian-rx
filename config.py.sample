from flask import Flask
from flask_pymongo import PyMongo
from flask_bcrypt import Bcrypt

app = Flask(__name__)
app.config['DEBUG'] = True
app.config['MONGO_DBNAME'] = 'db_name'
app.config['MONGO_URI'] = 'mongodb://localhost:27017/db_name'
app.secret_key = ''
mongo = PyMongo(app)
bcrypt = Bcrypt(app)
